{
    "name": "hmap",
    "version": "1.0.0",
    "description": "Die2Nite HTML5 replacement map",
    "scripts": {
        "build-ts": "tsc",
        "browserify": "browserify dist/ -o dist/hmap.user.js",
        "copy-headers": "echo \"$(cat header-userscript)\n$(cat dist/hmap.user.js)\" > dist/hmap.user.js",
        "build": "npm run build-ts && npm run browserify && npm run copy-headers",
        "lint": "tslint --project tslint.json ./src/*.ts"
    },
    "author": "RyderOne",
    "dependencies": {
        "@types/jquery": "^3.3.29"
    },
    "devDependencies": {
        "browserify": "^16.2.3",
        "tslint": "^5.14.0",
        "typescript": "^3.3.4000"
    }
}
